<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="316.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab_map_javafx.controller.UpdateController">
    <children>
        <TextField fx:id="updateUsername" layoutX="43.0" layoutY="71.0" prefHeight="26.0" prefWidth="154.0" promptText="Username" />
        <TextField fx:id="updateFirstName" layoutX="43.0" layoutY="106.0" prefHeight="26.0" prefWidth="154.0" promptText="New first name" />
        <TextField fx:id="updateLastName" layoutX="43.0" layoutY="145.0" prefHeight="26.0" prefWidth="154.0" promptText="New last name" />
        <Button fx:id="updateNameButton" layoutX="40.0" layoutY="201.0" mnemonicParsing="false" onAction="#updateNameButtonClicked" prefHeight="26.0" prefWidth="154.0" text="Change name" />
        <PasswordField fx:id="updateOldPassword" layoutX="256.0" layoutY="71.0" prefHeight="26.0" prefWidth="154.0" promptText="Old password" />
        <PasswordField fx:id="updateNewPassword" layoutX="256.0" layoutY="106.0" prefHeight="26.0" prefWidth="154.0" promptText="New password" />
        <PasswordField fx:id="updateConfirmNewPassword" layoutX="256.0" layoutY="145.0" prefHeight="26.0" prefWidth="154.0" promptText="Confirm new password" />
        <Button fx:id="updatePasswordButton" layoutX="256.0" layoutY="201.0" mnemonicParsing="false" onAction="#updatePasswordButtonClicked" prefHeight="26.0" prefWidth="154.0" text="Change password" />
        <Button fx:id="closeButton" layoutX="144.0" layoutY="267.0" mnemonicParsing="false" onAction="#closeButtonClicked" prefHeight="26.0" prefWidth="154.0" text="Close" />
    </children>
</AnchorPane>
